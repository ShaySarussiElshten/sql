# SQL Server Database Comparison Configuration - Config Object Style
# Copy this file to .env.config and update with your actual database credentials
# Use this instead of .env.docker for NestJS-style configuration

# Database 1 Configuration
DB1_HOST=your-server-host
DB1_PORT=1433
DB1_USER=your-username
DB1_PASSWORD=your-password
DB1_DATABASE=your-database1

# Database 2 Configuration  
DB2_HOST=your-server-host
DB2_PORT=1434
DB2_USER=your-username
DB2_PASSWORD=your-password
DB2_DATABASE=your-database2

# Queries - Update these to match your actual table structures
DB1_QUERY=SELECT TOP 1000 * FROM your_table1 ORDER BY id
DB2_QUERY=SELECT TOP 1000 * FROM your_table2 ORDER BY id

# Acceptable delta for numeric comparisons
ACCEPTABLE_DELTA=0.01
